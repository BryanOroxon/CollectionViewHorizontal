<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CollectionViewHorizontal.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CollectionViewHorizontal"
    xmlns:view="clr-namespace:CollectionViewHorizontal.Views"
    Title="CollectionViewHorizontal"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="HomePage"
        ContentTemplate="{DataTemplate view:SixColumnsPage}"
        Route="HomePage" />
    <ShellContent
        Title="Articulos"
        ContentTemplate="{DataTemplate view:ArticuloPage}"
        Route="ArticuloPage" />
    <ShellContent
        Title="Ubicacion"
        ContentTemplate="{DataTemplate view:UbicacionPage}"
        Route="UbicacionPage" />

</Shell>
